{
  "name": "@ncstat/parser",
  "version": "1.0.0",
  "description": "Static NC file parser and analyzer",
  "author": "Kevin Hill",
  "license": "MIT",
  "private": true,
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "test": "jest",
    "demo": "tsnd -r tsconfig-paths/register --clear --respawn ./demo",
    "demo:vf10": "tsnd -r tsconfig-paths/register ./demo/vf10.ts",
    "dev": "cross-env DEBUG=ncstat:* tsnd -r tsconfig-paths/register --clear --respawn ./demo",
    "start": "cross-env DEBUG=ncstat:* tsnd -r tsconfig-paths/register --clear --respawn ./demo",
    "build": "node ../../scripts/build.js",
    "test:NcProgram": "yarn test --watch NcProgram"
  },
  "dependencies": {
    "debug": "^4.1.1",
    "eventemitter3": "^4.0.0",
    "lodash": "^4.17.15",
    "purify-ts": "^0.15.1",
    "tokenizr": "^1.5.7",
    "ts-tokenizr": "*",
    "xstate": "^4.7.8"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/lodash": "^4.14.149",
    "@typescript-eslint/parser": "^3.2.0",
    "cross-env": "^7.0.2",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jest-formatting": "^1.2.0",
    "jest": "^26.0.1",
    "ts-jest": "^26.1.0",
    "ts-node-dev": "^1.0.0-pre.44",
    "tsconfig-paths": "^3.9.0"
  }
}
