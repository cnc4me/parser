{
  "extends": "../../.eslintrc.js",
  "plugins": ["import"],
  "settings": {
    "import/resolver": {
      "alias": {
        "map": [["@/*", "./src"]],
        "extensions": [".ts", ".js", ".jsx", ".json"]
      }
    }
  },
  "rules": {
    "no-restricted-globals": ["globalThis"],
    "import/no-extraneous-dependencies": "off"
  }
}
