{
  "name": "ncstat",
  "version": "1.0.0",
  "private": true,
  "bin": "./dist/cli.js",
  "main": "./dist/index.js",
  "module": "./dist/index.esm.js",
  "dependencies": {
    "@xstate/fsm": "^1.3.0",
    "clipanion": "^2.1.6",
    "concurrently": "^5.1.0",
    "doublie": "^1.1.0",
    "eventemitter3": "^4.0.0",
    "lodash": "^4.17.15",
    "tokenizr": "^1.5.7"
  },
  "scripts": {
    "ts": "ts-node -r tsconfig-paths/register",
    "cli": "yarn ts ./src/cli/index.ts",
    "cli:lex": "yarn cli lex",
    "cli:parse": "yarn cli parse",
    "build": "concurrently npm:build:*",
    "build:core": "bili ./src/index.ts --file-name ncstat.js",
    "build:cli": "bili -c bili.config.cli.js",
    "dev": "tsnd --respawn",
    "lint": "eslint --ext .ts ./src",
    "lint:fix": "yarn lint --fix",
    "test": "jest",
    "test:watch": "jest --notify --watch"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@types/jest": "^25.1.1",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.5.1",
    "@types/tokenizr": "^1.5.1",
    "@typescript-eslint/eslint-plugin": "^2.7.0",
    "@typescript-eslint/parser": "^2.11.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^25.1.0",
    "bili": "^4.8.1",
    "eslint": "^6.8.0",
    "eslint-config-node": "^4.0.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jest": "^23.1.1",
    "eslint-plugin-jest-formatting": "^1.2.0",
    "eslint-plugin-lodash-fp": "^2.2.0-a1",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-simple-import-sort": "^4.0.0",
    "husky": "^4.2.1",
    "jest": "^25.1.0",
    "lint-staged": ">=10",
    "lodash-es": "^4.17.15",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "rollup": "^1.30.1",
    "rollup-plugin-typescript2": "^0.25.3",
    "ts-jest": "^25.1.0",
    "ts-node": "^8.6.2",
    "ts-node-dev": "^1.0.0-pre.44",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.5"
  },
  "lint-staged": {
    "*.ts": "eslint --cache --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
