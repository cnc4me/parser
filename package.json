{
  "name": "nc-scanner",
  "version": "1.0.0",
  "description": "Scans NC program for static analysis",
  "bin": "./bin/analyze",
  "types": "./src/typings",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/harris-bruno/nc-scanner.git"
  },
  "author": "Kevin Hill",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://gitlab.com/harris-bruno/nc-scanner/issues"
  },
  "homepage": "https://gitlab.com/harris-bruno/nc-scanner#readme",
  "scripts": {
    "test": "ava",
    "lint": "tslint 'src/**/*.ts'",
    "lint:fix": "tslint --fix 'src/**/*.ts'",
    "build": "tsc",
    "watch": "tsc --watch",
    "example1": "node --no-warnings ./bin/analyze --canned-cycles ./nc/example1.NC",
    "example2": "node --no-warnings ./bin/analyze --canned-cycles ./nc/example2.NC",
    "start": "./bin/nc-scanner /mnt/c/\"H+405 PROGRAM VAULT\" -w \"HB PARTS,JOB SPECIFIC\""
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "commander": "^2.19.0",
    "debug": "^4.1.1",
    "fs-extra": "^7.0.1",
    "javascript-state-machine": "^3.1.0",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.11"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.134",
    "@types/node": "^11",
    "ava": "^2.0.0",
    "prettier": "1.18.2",
    "ts-node": "^8.2.0",
    "tslint": "^5.17.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.5.1"
  }
}
